{
  "version": 2,
  "bunVersion": "1.x",
  "buildCommand": "cd packages/app && bun run build:vercel",
  "installCommand": "bun install",
  "outputDirectory": "packages/app/.next",
  "framework": "nextjs",
  "functions": {
    "packages/app/api/**/*.{js,ts}": {
      "memory": 2048,
      "maxDuration": 60
    }
  }
}